{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node",
      "regions": ["hnd1"]
    },
    {
      "src": "front/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/posts(/.*)?",
      "dest": "api/server.js"
    },
    {
      "src": "/front/(.*)",
      "dest": "/front/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}