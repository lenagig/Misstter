{
  "version": 2,
  "builds": [
    {
      "src": "back/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "../index.html",
          "../front/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "back/server.js"
    }
  ]
}